{% extends "/sacrud/redefineme.jinja2" %}

{% block sa_body %}
    <div class="page">
      {% if sa_crud['table'].sacrud_list_template %}
        {% include sa_crud['table'].sacrud_list_template with context %}
      {% else %}
        {% include '/sacrud/forms/list.jinja2' with context %}
      {% endif %}
    </div>
{% endblock %}

{% block sa_head_js %}
    <script type="text/javascript" src="{{ request.static_url('sacrud:static/js/jquery/jquery-1.9.1.min.js') }}"></script>
{% endblock %}

{% block extrascript %}
    <script type="text/javascript" src="{{ request.static_url('sacrud:static/js/jquery/jquery-ui-1.10.4.custom.min.js') }}"></script>
    <script type="text/javascript" src="{{ request.static_url('sacrud:static/js/selectable.js') }}"></script>
{% endblock %}
